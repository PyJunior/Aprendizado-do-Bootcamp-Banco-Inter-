escreval("Qualquer outro numero, voce sera redirecionado ao menu inicial")
escreval("                                                                 ")
escreval("                                                                 ")
Leia(A)
se A=1 entao
   escreva("Voce está no atendimento ao cliente JR compras")
   escreva(" nos informe sua duvida!")
senao
se A=2 entao
   escreva("Diga qual sera a sua forma de pagamento")
senao
se A=3 entao
