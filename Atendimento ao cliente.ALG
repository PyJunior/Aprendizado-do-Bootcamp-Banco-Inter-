escreval("para darmos continuidade ao atendimento.")
escreval("                                                   ")
Leia(Nome)
escreval("                       ")
escreval("[1] Masculino")
escreval("[2] Feminino")
escreval("")
escreval("")
Leia(S)
se S=1 entao
escreval("Seja bem vindo senhor ",Nome," como podemos ajuda-lo?")
senao
escreval("Seja bem vinda senhora ",Nome," como podemos ajuda-la?")
fimse
escreval("[1] para atendimento com o cliente")
